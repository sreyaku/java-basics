public class PracticeIfElse {
	public static void main(String args[]) {
		//int n=0;
		Scanner sc=new Scanner(System.in);  
		System.out.print("Enter a number: ");  
		int n = sc.nextInt(); 
			if(n==0)
				System.out.println("Even");
			else if(n%2==0)
				System.out.println("Even");
			else
				System.out.println("Odd");
					
	}

}
