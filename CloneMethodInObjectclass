import java.util.GregorianCalendar;

public class CloneMethodInObjectclass {

   public static void main(String[] args) {

      // create a gregorian calendar, which is an object
      GregorianCalendar calender = new GregorianCalendar();

      // clone object cal into object y
      GregorianCalendar clonecalender = (GregorianCalendar) calender.clone();

      // print both calendar  and clonecalender
      System.out.println("" + calender.getTime());
      System.out.println("" + clonecalender.getTime());
   }
}
